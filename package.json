{
  "name": "@npkgdev/nestjs-request-context",
  "version": "1.0.0",
  "description": "Helper library for accessing request from singleton services",
  "author": "Douglas de Paula Natalli",
  "license": "MIT",
  "url": "https://github.com/npkgdev/nestjs-request-context#readme",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access private",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access private --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "npx semantic-release"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-angular": "16.0.0",
    "@nestjs/common": "8.2.5",
    "@nestjs/core": "8.2.5",
    "@nestjs/platform-express": "8.2.5",
    "@nestjs/testing": "8.2.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.20",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.2.0",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.2",
    "semantic-release": "^19.0.2",
    "supertest": "6.2.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.4"
  },
  "dependencies": {},
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "express": "^4.17.2",
    "reflect-metadata": "^0.1.13"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "publishConfig": {
    "@npkgdev:registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/npkgdev/nestjs-request-context"
  }
}
